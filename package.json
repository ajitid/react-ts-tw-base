{
  "name": "react-ts-tw-base",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src/**/*.{js,ts,jsx,tsx,mdx} --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src/**/*.{js,ts,jsx,tsx,md,mdx,css,json}",
    "preview": "vite preview",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  },
  "engines": {
    "node": ">=20.16.0",
    "pnpm": ">=9.6.0"
  }
}
